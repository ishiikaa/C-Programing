#include <stdio.h>

int main()
{
    int a;
    int i, prime;
    
    printf("Enter a number: ");
    scanf("%d", &a);
    
    for(i=2; i<=a; i++){
        if(a % i == 0){
            prime = 0;           
            }    
    } 
        if(prime == 0)
        printf(" %d is not a prime number", a);
        
        else
        printf("%d is a prime number", a);
        
    return 0;
}
